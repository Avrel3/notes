{
  "name": "notes",
  "version": "1.0.0",
  "homepage": "https://avrel3.github.io/notes",
  "scripts": {
    "start": "parcel src/index.html --no-cache --dist-dir out --cache-dir .cache --open --public-url ./",
    "build": "parcel build src/index.html --dist-dir build --no-cache --cache-dir .cache --no-source-maps --public-url ./",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.65",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "gh-pages": "^4.0.0",
    "parcel": "^2.2.1",
    "process": "^0.11.10",
    "typescript": "^4.8.4"
  }
}
